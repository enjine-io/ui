      "de": "",
      "es": ""
    },
    "subtitle_107": {
      "en": "getEnabledByName",
      "de": "",
      "es": ""
    },
    "text_108": {
      "en": "Get the enabled state of the bottom navigation action by its name.",
      "de": "",
      "es": ""
    },
    "subtitle_112": {
