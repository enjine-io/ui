      "de": "",
      "es": ""
    },
    "text_14": {
      "en": "var navs = [",
      "de": "",
      "es": ""
    },
    "text_15": {
      "en": "[ \"Favorites\", \"favorites\" ],",
      "de": "",
      "es": ""
    },
    "text_16": {
      "en": "[ \"Groups\", \"person\" ],",
      "de": "",
      "es": ""
    },
    "text_17": {
      "en": "[ \"Folder\", \"folder\" ]",
      "de": "",
      "es": ""
    },
    "text_18": {
